{
    "name": "链接文本",
    "icon": "iconLinktext",
    "type":"element",
    "staticProps": [
        {
            "name": "文本",
            "prop": "text",
            "size": "long"
        },
        {
            "name": "文字颜色",
            "prop": "color",
            "type": "color"
        },
        {
            "name": "文字大小",
            "prop": "fontSize"
        },
        {
            "name": "文字加粗",
            "prop": "fontWeight",
            "type": "select",
            "option": [
                {
                    "label": "加粗",
                    "value": "bold"
                },
                {
                    "label": "常规",
                    "value": "normal"
                }
            ]
        },
        {
            "name": "行高",
            "prop": "lineHeight"
        },
        {
            "name": "对齐方式",
            "prop": "textAlign",
            "type": "select",
            "option": [
                {
                    "label": "居中",
                    "value": "center"
                },
                {
                    "label": "左对齐",
                    "value": "left"
                },
                {
                    "label": "右对齐",
                    "value": "right"
                }
            ]
        },
        {
            "name": "点击链接",
            "prop": "link",
            "size": "long"
        },
        {
            "name": "是否在新窗口打开链接",
            "prop": "blank",
            "type": "switch",
            "size": "long"
        }
    ],
    "defaultStyles": {
        "position": "relative",
        "width": "200px",
        "height": "30px"
    },
    "defaultProps": {
        "text": "链接文本",
        "fontSize": "20px",
        "lineHeight": "36px",
        "textAlign": "center",
        "blank": true,
        "href": "http://www.baidu.com"
    },
    "hasChild": false,
    "canResizeByMouse": true
}